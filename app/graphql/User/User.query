query Users {
  users {
    id
    name
    telegramId
  }
}

query User {
  user(telegramId: 462566829) {
    id
    name
    telegramId
    email
    phone
  }
}

mutation DeleteUser {
  deleteUser(userId: "33d36cde-454f-41f2-b949-26034e96d065")
}

mutation CreateUser {
  createUser(userInfo: {name: "Provider", telegramId: 42})
}

mutation UpdateUser {
  updateUser(
    telegramId: 42
    userInfo: {email: "shiningfinger1@yandex.ru"}
  ) {
    telegramId
    email
    phone
  }
}