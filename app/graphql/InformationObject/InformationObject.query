query InformationObjects {
  informationObjects {
    id
    name
    description
    gallery {
      id
      url
    }
    providerId
  }
}

query InformationObject {
   informationObject(id: "") {
      id
      name
      description
      gallery {
        id
        url
      }
      provider {
        id
        user {
          name
          username
          telegramId
        }
      }
    }
}

mutation DeleteInformationObject {
  deleteInformationObject(id: "")
}

